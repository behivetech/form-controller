    },
    "peerDependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    },
    "scripts": {
        "deploybuild": "webpack --config webpack.config.prod.js",
        "transpile": "babel src --out-file dist/index.js --extensions \".ts,.tsx\" --source-maps inline",
